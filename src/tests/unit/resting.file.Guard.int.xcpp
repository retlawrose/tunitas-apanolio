#divert <ipp>
#import sys.posix.close
template<> tests::unit::resting::file::Guard<tests::unit::resting::file::Pair<int>>::~Guard() {
  // Try our best ... don't even report the failures
  using ::sys::posix::close;
  close(subject.first);
  close(subject.second);
}
#endiv
