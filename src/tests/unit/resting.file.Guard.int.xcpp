#divert <hpp>
#import tests.unit.resting.file.Guard
#import tests.unit.resting.file.Pair
namespace tests {
  namespace unit {
    namespace resting {
      namespace file {
        //
        // Upon destruction, close both file descriptors mentioned in the subject
        //
        template<> inline Guard<Pair<int>>::~Guard();
      }
    }
  }
}
